{
    "name": "cross_roads",
    "version": "2.0.0",
    "description": "Need to cross the road without colliding with a moving vehicle.",
    "packageManager": "npm@10.7.0",
    "type": "module",
    "dependencies": {
        "@drk4/utilities": "^6.2.0",
        "easeljs": "^1.0.2",
        "preloadjs": "^1.0.1",
        "soundjs": "^1.0.1",
        "tweenjs": "^1.0.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.6.0",
        "@types/createjs": "~0.0.29",
        "eslint": "^9.6.0",
        "globals": "^15.6.0",
        "prettier": "^3",
        "typescript": "^5",
        "typescript-eslint": "^7.14.1",
        "vite": "^5"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "always",
        "htmlWhitespaceSensitivity": "strict",
        "endOfLine": "lf"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format:check": "prettier --check \"**/*.{html,css,json,js,ts,md}\"",
        "format:run": "prettier --write \"**/*.{html,css,json,js,ts,md}\"",
        "check:types": "tsc -p tsconfig.json --noEmit --skipLibCheck",
        "lint": "eslint source"
    }
}
